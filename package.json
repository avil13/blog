    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "purify-css": "^1.2.5",
    "terser": "^4.7.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "yaspeller": "^7.0.0"
  }
